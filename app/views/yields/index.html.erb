<p id="notice"><%= notice %></p>

<h1>Yields</h1>

<table>
  <thead>
  <tr>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @yields.each do |object_yield| %>
    <tr>
      <td><%= link_to 'Show', object_yield %></td>
      <td><%= link_to 'Edit', edit_object_yield_path(object_yield) %></td>
      <td><%= link_to 'Destroy', object_yield, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Yield', new_yield_path %>
